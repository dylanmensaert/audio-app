<?xml version='1.0' encoding='utf-8'?>
<widget id="com.embercordova.audioApp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>audioApp</name>
    <description>
    </description>
    <content src="index.html" />
    <plugin name="cordova-plugin-file" version="4.3.0" />
    <plugin name="cordova-plugin-network-information" version="1.3.0" />
    <plugin name="cordova-plugin-whitelist" version="1.3.0" />
    <plugin name="cordova-plugin-music-controls" version="1.4.1" />
    <access origin="*" />
    <access origin="cdvfile://*" />
    <allow-intent href="*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~6.1.2" />
    <plugin name="cordova-plugin-crosswalk-webview" spec="~2.3.0">
        <variable name="XWALK_VERSION" value="23+" />
        <variable name="XWALK_LITEVERSION" value="xwalk_core_library_canary:17+" />
        <variable name="XWALK_COMMANDLINE" value="--disable-pull-to-refresh-effect" />
        <variable name="XWALK_MODE" value="embedded" />
        <variable name="XWALK_MULTIPLEAPK" value="true" />
    </plugin>
</widget>
