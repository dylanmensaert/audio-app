<div {{action 'click'}} class="card-image waves-effect waves-block waves-light">
    {{c-image src=model.thumbnail}}
</div>

<div class="card-content my-playlist-content">
  <div class="row">
    <div class="truncate">
      {{model.name}}
    </div>

    <i class="material-icons" {{action 'changeSelect'}}>more_vert</i>
  </div>
</div>

{{#if model.isSaved}}
  <div class="card-action">
      <i class="material-icons">shop_two</i>
  </div>
{{/if}}
