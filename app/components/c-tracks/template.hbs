{{c-track-actions models=selectedTracks playlist=playlist}}

{{#c-endless-scroll didScrollToBottom='didScrollToBottom' }}
    {{#each model.tracks as |track|}}
        {{c-track model=track hideDownloaded=model.isDownloaded play='playTrack'}}
    {{/each}}

    {{#if isPending}}
        {{c-spinner class='my-spinner'}}
    {{/if}}
{{/c-endless-scroll}}